# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Blockchain Configuration
VITE_CHAIN_ID=11155111
VITE_CHAIN_NAME=Sepolia
VITE_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# ============================================================================
# KYC & Compliance Configuration (Phase 6)
# ============================================================================

# KYC Provider Selection
# Options: sumsub, synaps, onfido, civic
KYC_PROVIDER=sumsub

# Sumsub Configuration
# Get these from Sumsub Dashboard: https://dashboard.sumsub.com/
KYC_PROVIDER_API_KEY=your_sumsub_app_token
KYC_PROVIDER_SECRET=your_sumsub_secret_key
SUMSUB_BASE_URL=https://api.sumsub.com
SUMSUB_LEVEL_NAME=basic-kyc-level

# Webhook Configuration
# Set this URL in your Sumsub dashboard webhook settings
KYC_CALLBACK_URL=https://your-project.supabase.co/functions/v1/kyc-webhook

# Geographic / IP Configuration
# Optional: ipinfo.io API key for higher rate limits
GEOIP_API_KEY=

# Data Retention
# Number of days to retain non-approved KYC data before purging
KYC_RETENTION_DAYS=365

# Optional: Custom sanctions list URL
# SANCTIONS_LIST_URL=

# ============================================================================
# End KYC Configuration
# ============================================================================

# Optional: Analytics, etc.
# VITE_GA_ID=your_google_analytics_id

